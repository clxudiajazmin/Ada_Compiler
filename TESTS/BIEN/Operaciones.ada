procedure Main is
   A    : Integer := 0;
   B    : Integer := 4;
   C    : Integer := 100;
   D,E,F,G  : Integer;
   H, I : Float;
   K : Float := 9.7;
   J : Float := 7.2;
begin
   A := A + 1;

   D := A + B + C;
   E := A - B + C;
   F := A * B * C;
   G := A / B;

   H := K * J;
   I := K + J;
end Main;